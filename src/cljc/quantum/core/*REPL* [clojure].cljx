    [quantum.core.reducers :as red :refer
      #?@(:clj  [[map+ reduce+ filter+ remove+ take+ take-while+ taker+ dropr+
                  count* fold+ range+ drop+ for+]]
          :cljs [[map+ reduce+ filter+ remove+ take+ take-while+
