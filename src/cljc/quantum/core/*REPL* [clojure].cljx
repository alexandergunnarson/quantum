(defprotocol CollMod
  (conjl-
    #?@(:clj ([coll a]
              [coll a b]
              [coll a b c]
              [coll a b c d] 
              [coll a b c d e])
       :cljs ([coll args])))
  (conjr-
    #?@(:clj  ([coll arg] [coll arg & args])
       :cljs ([coll args]))))

