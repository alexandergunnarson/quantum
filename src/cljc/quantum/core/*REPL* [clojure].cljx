(extend-protocol CollRetrieve
  ; sorted-map or sorted-set getr+ like subseq
 #?(:clj  Object ; "default"
    :cljs default)
    (get+
      ([obj n]              (get+ obj n nil))
      ([obj n if-not-found] (get obj n)))
    (first+   [obj] obj)
    (last+    [obj] obj)
    (butlast+ [obj] obj)
  nil
    (get+     [obj n] obj)
    (first+   [obj]   obj)
    (last+    [obj]   obj)
    (butlast+ [obj]   obj))

(defn gets+ [coll & indices]
  (if (should-transientize? indices)
      (persistent!
        (reduce
          (fn [ret ind] (conj! ret (get+ coll ind)))
          (transient [])
          indices))
      (reduce
        (fn [ret ind] (conj ret (get+ coll ind)))
        []
        indices)))

(def  pop+  butlast+)
(def  popr+ butlast+)
(def  popl+ rest+)
(def  peek+ last+)

(defn getf+ [n] (f*n get+ n))
